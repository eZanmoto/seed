person := {
    "_age": 20,

    "age": fn () {
        return this._age
    },
}

print(person.age())
--------------------------------------------------
20
================================================== write
person := {
    "_age": 20,

    "inc_age": fn () {
        this._age = this._age + 1
    },

    "age": fn () {
        return this._age
    },
}

print(person.age())
person.inc_age()
print(person.age())
print(person._age)
--------------------------------------------------
20
21
21
================================================== bind
person := {
    "_age": 10,

    "set_age": fn (age) {
        this._age = age
    },
}

set_age := person.set_age

set_age(20)
print(person._age)
--------------------------------------------------
20
