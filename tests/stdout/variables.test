================================================== range_index_reference
xs := [{"a": 1}, {"b": 2}];
ys := [null, null];
ys[0:2] = xs;
ys[0].a = 2;
ys[1].b = 3;
print(xs);
--------------------------------------------------
[
    {
        "a": 2,
    },
    {
        "b": 3,
    },
]
